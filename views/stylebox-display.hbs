
<div class="demand-page hidden full-page">
  {{> demand}}
</div>

<div class="availabilities-page hidden full-page">
  {{> availabilities}}
</div>

<div class="message-page hidden full-page">
  {{> message}}
</div>

{{#if user}}
  {{> userheader}}
{{else}}
  {{> header}}
{{/if}}


<div class="col-md-10 col-md-offset-1 col-sm-12 col-xs-12 stylist-display-container padding-zero">
  <div class="col-md-3 col-sm-4 col-xs-12 padding-zero stylist-left-side-container" style="margin-bottom:40px;">
    <div class="col-md-12 col-sm-12 col-xs-12 padding-zero stylist-left-side">
      <div class="col-md-12 col-sm-12 col-xs-12 padding-zero">
        <div class="stylebox-display-avatar" style="background-image: url({{stylebox.avatar}})"></div>
        <div class="col-md-4 col-md-offset-8 col-sm-4 col-sm-offset-8 col-xs-4 col-xs-offset-8 stylistPrice stylistPrice">
          <p>{{stylebox.price}}€ / h</p>
        </div>
      </div>
      <div class="col-md-12 col-sm-12 hidden-xs pricing-btns padding-zero send-message-btn text-center">
        <i class="fa fa-envelope-o pricer-menu-icons"></i>
        <p class="pricer-p">Contacter</p>
      </div>
      <div class="col-md-12 col-sm-12 hidden-xs padding-zero demand-btn-container">
        <button class="btn demand-btn send-demand-btn" type="button">Prendre rendez-vous</button>
      </div>
    </div>
    <div class="col-md-5 col-sm-6 col-xs-6 padding-zero text-left">
      <p>{{stylebox.city}}</p>
    </div>
    <div class="col-md-6 col-sm-5 col-xs-5 padding-zero text-right">
      <select class="general-rating">
        <option value="1">1</option>
        <option value="2">2</option>
        <option value="3">3</option>
        <option value="4">4</option>
        <option value="5">5</option>
      </select>
    </div>
    <div class="col-md-1 col-sm-1 col-xs-1 text-right padding-zero">
      <p>({{stylebox.number}})</p>
    </div>

    <div class="col-md-12 col-sm-12 col-xs-12 facebook-share">
      <div class="fb-share-button" data-href="https://fason.co/stylebox/{{styleboxId}}" data-layout="button" data-size="large" data-mobile-iframe="true"><a class="fb-xfbml-parse-ignore" target="_blank" href="https://www.facebook.com/sharer/sharer.php?u=https%3A%2F%2Ffason.co%2F&amp;src=sdkpreparse">Partager</a></div>
    </div>
  </div>
  <div class="col-md-9 col-sm-8 col-xs-12 stylist-infos-container">

    <div class="col-md-12 col-sm-12 col-xs-12 stylist-infos-row">
      <h4 style="margin-top:0;">À propos de {{stylebox.lastName}}</h4>
      {{#eachNewLine stylebox.description}}
    		<p>{{this}}</p>
    	{{/eachNewLine}}
    </div>

    <div class="col-md-12 col-sm-12 col-xs-12 stylist-infos-row notes-container">
      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 review-mobile-columns padding-zero text-left">
          <h4 class="notesh4">Notes</h4>
      </div>
        <div class="col-lg-6 col-md-6 col-sm-12 col-sm-12 col-xs-12 review-mobile-columns padding-zero text-left">
            <div class="row general-row">
                <div class="col-lg-6 col-md-6 col-xs-6 general-column">
                    <p class="notes-text inlinep">Précision :</p>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 general-column">
                  <select class="precision-rating">
                    <option value="1">1</option>
                    <option value="2">2</option>
                    <option value="3">3</option>
                    <option value="4">4</option>
                    <option value="5">5</option>
                  </select>
                </div>
            </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 review-mobile-columns padding-zero text-left">
            <div class="row general-row">
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 general-column">
                    <p class="notes-text inlinep">Qualité prix :</p>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 general-column">
                  <select class="quality-rating">
                    <option value="1">1</option>
                    <option value="2">2</option>
                    <option value="3">3</option>
                    <option value="4">4</option>
                    <option value="5">5</option>
                  </select>
                </div>
            </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 review-mobile-columns padding-zero text-left">
            <div class="row general-row">
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 general-column">
                    <p class="notes-text inlinep">Communication : </p>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 general-column">
                  <select class="communication-rating">
                    <option value="1">1</option>
                    <option value="2">2</option>
                    <option value="3">3</option>
                    <option value="4">4</option>
                    <option value="5">5</option>
                  </select>
                </div>
            </div>
        </div>
        <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12 review-mobile-columns padding-zero text-left">
            <div class="row general-row">
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 general-column">
                    <p class="notes-text inlinep">Ponctualité : </p>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 general-column">
                  <select class="ponctuality-rating">
                    <option value="1">1</option>
                    <option value="2">2</option>
                    <option value="3">3</option>
                    <option value="4">4</option>
                    <option value="5">5</option>
                  </select>
                </div>
            </div>
        </div>
    </div>

    <div class="col-md-12 col-sm-12 col-xs-12 stylist-infos-row">
      <h4 class="notesh4">Étapes de relooking</h4>
      {{#eachNewLine stylebox.generalprocess}}
    		<p>{{this}}</p>x
    	{{/eachNewLine}}
    </div>


    <div class="col-md-12 col-sm-12 col-xs-12 stylist-infos-row">
      <h4 class="notesh4">Services relooking vestimentaire</h4>
      <div {{#if stylebox.relooking.0.ok}} class="on col-md-6 col-sm-12 col-xs-12 padding-zero" {{else}} class="off col-md-6 col-sm-12 col-xs-12 padding-zero" {{/if}}>
        <p data-id="reve-tourmagville">Shopping à votre place</p>
      </div>
      <div {{#if stylebox.relooking.1.ok}} class="on col-md-6 col-sm-12 col-xs-12 padding-zero" {{else}} class="off col-md-6 col-sm-12 col-xs-12 padding-zero" {{/if}}>
        <p data-id="reve-tourmagville">Accompagnement shopping</p>
      </div>
      <div {{#if stylebox.relooking.2.ok}} class="on col-md-6 col-sm-12 col-xs-12 padding-zero" {{else}} class="off col-md-6 col-sm-12 col-xs-12 padding-zero" {{/if}}>
        <p data-id="reve-tourmagville">Tri de la garde-robe</p>
      </div>
      <div {{#if stylebox.relooking.3.ok}} class="on col-md-6 col-sm-12 col-xs-12 padding-zero" {{else}} class="off col-md-6 col-sm-12 col-xs-12 padding-zero" {{/if}}>
        <p data-id="reve-tourmagville">Faire un tour des prêts-à-porter</p>
      </div>
    </div>

    <div class="col-md-12 col-sm-12 col-xs-12 stylist-infos-row">
      <h4 class="notesh4">Services relooking beauté</h4>
      <div {{#if stylebox.beaute.0.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="beau-maqui">Maquillage</p>
      </div>
      <div {{#if stylebox.beaute.1.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="beau-coiff">Coiffure</p>
      </div>
      <div {{#if stylebox.beaute.2.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="beau-color">Coloration</p>
      </div>
      <div {{#if stylebox.beaute.3.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="beau-manuc">Manucure</p>
      </div>
      <div {{#if stylebox.beaute.4.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="beau-pedic">Pédicure</p>
      </div>
      <div {{#if stylebox.beaute.5.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="beau-surci">Sourcils</p>
      </div>
      <div {{#if stylebox.beaute.6.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="beau-soinviz">Soins du visage</p>
      </div>
      <div {{#if stylebox.beaute.7.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="beau-soincor">Soins du corps</p>
      </div>
      <div {{#if stylebox.beaute.8.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="beau-tailbarb">Taille de barbe</p>
      </div>
    </div>
    <div class="col-md-12 col-sm-12 col-xs-12 stylist-infos-row">
      <h4 class="notesh4">Cours de relooking</h4>
      <div {{#if stylebox.corses.0.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="cour-maqui">Maquillage</p>
      </div>
      <div {{#if stylebox.corses.1.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="cour-coiff">Coiffure</p>
      </div>
      <div {{#if stylebox.corses.2.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="cour-color">Coloration</p>
      </div>
      <div {{#if stylebox.corses.3.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="cour-manuc">Manucure</p>
      </div>
      <div {{#if stylebox.corses.4.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="cour-pedic">Pédicure</p>
      </div>
      <div {{#if stylebox.corses.5.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="cour-surci">Sourcils</p>
      </div>
      <div {{#if stylebox.corses.6.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="cour-soinviz">Soins du visage</p>
      </div>
      <div {{#if stylebox.corses.7.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="cour-soincor">Soins du corps</p>
      </div>
      <div {{#if stylebox.corses.8.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="cour-tailbarb">Taille de barbe</p>
      </div>
      <div {{#if stylebox.corses.9.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="cour-couleur">Couleurs</p>
      </div>
      <div {{#if stylebox.corses.10.ok}} class="on col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{else}} class="off col-md-4 col-sm-6 col-xs-6 padding-zero padding-zero" {{/if}}>
        <p data-id="cour-morpho">Morphologie</p>
      </div>
    </div>
    <div class="col-md-12 col-sm-12 col-xs-12 stylist-infos-row">
      <h4 class="notesh4">Femme / homme</h4>
      {{#if stylebox.men}}
        {{#if stylebox.women}}
        <p>Relooking femme et homme</p>
        {{else}}
        <p>Relooking homme uniquement</p>
        {{/if}}
      {{else}}
        {{#if stylebox.women}}
        <p>Relooking femme uniquement</p>
        {{/if}}
      {{/if}}

    </div>
  </div>
</div>

<div class="row commentRow">
  {{#if styleboxcomments}}
    <div class="commentTitle">
      <h4>Commentaires</h4>
    </div>
    {{#each styleboxcomments}}
      <div class="row commentrow">
        <div class="col-lg-2 col-md-2 col-sm-2 col-xs-3 text-center">
          <div class="commentOwnerAva" style="background-image: url({{this.creatorAva}})">
          </div>
          <p>{{this.creatorName}}</p>
        </div>
        <div class="col-lg-10 col-md-10 col-sm-10 col-xs-9">
          <p class="commentbody">{{this.commentBody}}</p>
        </div>
      </div>
      <div class="row text-right commentCreatedDate">
        <p>{{this.createdDate}}</p>
      </div>
    {{/each}}
  {{/if}}
</div>
<div class="visible-xs-block xs-fixed-menu">
  <div class="row visible-xs-block general-row">
      <div class="col-xs-12 text-center xs-contac-menu general-column">
          <button class="btn btn-default pricer-footer-btns send-message-btn" type="button">Contacter</button>
      </div>
      <div class="col-xs-12 text-center general-column">
          <button class="btn btn-default demand-btn send-demand-btn pricer-footer-btns" type="button"> Prendre rendez-vous</button>
      </div>
  </div>
</div>

<div class="hidden-xs">
  {{> footer}}
</div>

