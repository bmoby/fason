<script type="text/javascript">
   $(function() {
      $('.general-rating').barrating({
        theme: 'fontawesome-stars-o',
        initialRating: {{{stylebox.rating.general}}},
        readonly: true
      });

      $('.precision-rating').barrating({
        theme: 'fontawesome-stars-o',
        initialRating: {{{stylebox.rating.precision}}},
        readonly: true
      });

      $('.ponctuality-rating').barrating({
        theme: 'fontawesome-stars-o',
        initialRating: {{{stylebox.rating.ponctuality}}},
        readonly: true
      });

      $('.quality-rating').barrating({
        theme: 'fontawesome-stars-o',
        initialRating: {{{stylebox.rating.quality}}},
        readonly: true
      });

      $('.communication-rating').barrating({
        theme: 'fontawesome-stars-o',
        initialRating: {{{stylebox.rating.communication}}},
        readonly: true
      });
   });
</script>


