
    <div class="container">
        <div class="notationdiv">
            <div class="row">
                <div class="col-md-12 text-right">
                    <p>{{this.demandTime}}</p>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-1 col-md-2 col-sm-2 col-xs-3 text-center">
                    <div class="eval-avatar" {{#if this.stylistAvatar}} style="background-image: url({{this.stylistAvatar}})" {{else}} style="background-image: url(/images/noavatar.png)"{{/if}}></div>

                </div>
                <div class="col-lg-11 col-md-10 col-sm-10 col-xs-9 text-center">
                    <p class="paragraphzero leaveReview">Evaluer {{this.stylistName}}</p>
                    <p class="paragraphzero">Look: {{this.styleboxTitle}}</p>
                    <p class="paragraphzero">Le: {{this.makeover}}</p>
                </div>
            </div>
            <div class="row stylebox-display-info evaluerblockpadding">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <p>Notez sur 5</p>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 review-mobile-columns text-left">
                    <div class="row general-row">
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 general-column">
                            <p class="notes-text inlinep">Précision :</p>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 general-column">
                          <select class="precision-rating rating">
                            <option value="1">1</option>
                            <option value="2">2</option>
                            <option value="3">3</option>
                            <option value="4">4</option>
                            <option value="5">5</option>
                          </select>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 review-mobile-columns text-left">
                    <div class="row general-row">
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 general-column">
                            <p class="notes-text inlinep">Qualité prix :</p>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 general-column">
                          <select class="quality-rating rating">
                            <option value="1">1</option>
                            <option value="2">2</option>
                            <option value="3">3</option>
                            <option value="4">4</option>
                            <option value="5">5</option>
                          </select>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 review-mobile-columns text-left">
                    <div class="row general-row">
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 general-column">
                            <p class="notes-text inlinep">Communication : </p>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 general-column">
                          <select class="communication-rating rating">
                            <option value="1">1</option>
                            <option value="2">2</option>
                            <option value="3">3</option>
                            <option value="4">4</option>
                            <option value="5">5</option>
                          </select>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12 review-mobile-columns text-left">
                    <div class="row general-row">
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 general-column">
                            <p class="notes-text inlinep">Ponctualité : </p>
                        </div>
                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6 general-column">
                          <select class="ponctuality-rating rating">
                            <option value="1">1</option>
                            <option value="2">2</option>
                            <option value="3">3</option>
                            <option value="4">4</option>
                            <option value="5">5</option>
                          </select>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <p class="leaveComment">Laissez votre commentaire :</p>
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 general-row">
                    <textarea class="mainLoginInput eval-comment-text" placeholder="Dites ce que vous en pensez de cette expérience : les points forts et les points faibles de votre relooker."></textarea>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 general-row">
                    <button class="btn btn-default demand-btn" id="evaluateStylistBtn" type="button" data-stylebox-id="{{this.styleboxId}}" data-stylist-id="{{this.stylistId}}" data-eval-id="{{this.evalId}}">Laisser mon avis</button>
                </div>
            </div>
        </div>
    </div>
